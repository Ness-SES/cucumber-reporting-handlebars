body {
    width:99%;
}
#feature-container {
    padding: 50px;
}
table tr {
    height: 25px;
}
#charts {
    padding 15px;
}
#charts div {
    float: left;
    width: 50%;
}
.back-to-top {
    background: none;
    margin: 0;
    position: fixed;
    bottom: 0;
    right: 0;
    width: 70px;
    height: 70px;
    z-index: 100;
    display: none;
    text-decoration: none;
}
.back-to-top i {
    font-size: 60px;
}